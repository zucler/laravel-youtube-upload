{
    "name": "zucler/laravel-youtube-upload",
    "description": "Upload a video to YouTube with Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Pak",
            "email": "zucler@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "^5.4|^6.0|^7.0|^8.0",
        "google/apiclient": "^2.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.2",
        "phpunit/phpunit": "~5.0",
        "phpunit/phpcov": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Dawson\\Youtube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dawson\\Youtube\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Dawson\\Youtube\\YoutubeServiceProvider"
            ],
            "aliases": {
                "Youtube": "Dawson\\Youtube\\Facades\\Youtube"
            }
        }
    }
}
