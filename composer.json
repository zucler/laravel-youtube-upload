{
    "name": "zucler/laravel-youtube-upload",
    "description": "Upload a video to YouTube with Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Pak",
            "email": "zucler@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "google/apiclient": "^2.2"
    },
    "require-dev": {
        "orchestra/testbench": "^8.5",
        "phpunit/phpunit": "^10.2",
        "phpunit/phpcov": "^9.0.0",
        "illuminate/support": "^10.13"
    },
    "autoload": {
        "psr-4": {
            "Dawson\\Youtube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dawson\\Youtube\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Dawson\\Youtube\\YoutubeServiceProvider"
            ],
            "aliases": {
                "Youtube": "Dawson\\Youtube\\Facades\\Youtube"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
